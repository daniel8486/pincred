            <div class="row">
              <div class="col l6 s12">
                <h5 class="white-text">  </h5>
                <p class="grey-text text-lighten-4">
                <ul>
                 <li> <a class="white-text"> <%= content.title  %> </a> </li> 
                 <li> <a class="white-text"> <%= content.address %> </a> </li>
