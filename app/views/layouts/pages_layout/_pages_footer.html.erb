                <h5 class="white-text">  </h5>
                <ul>
                  <li> <a class="white-text"> <%= content.phone_two %> </a></li>
                  <li> <a class="white-text"> <%= content.email %> </a></li>
                
                </ul>
              </div>
            </div>
          </div>
          <% end %> 
